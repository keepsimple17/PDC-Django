{
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": "off",
    "func-names": ["error", "never"],
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
    "camelcase": "off",
    "quotes": "off",
    "object-curly-spacing": "off",
    "no-else-return": "off"
  }
}
